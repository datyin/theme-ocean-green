  "tokenColors": [
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#b46363"
      }
    },
    {
      "scope": [
        "meta.brace",
        "punctuation",
        "keyword.operator.class",
        "keyword.operator.type.annotation",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#375860FF"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
      ],
      "settings": {
        "foreground": "#4A6C74FF"
      }
    },
    {
      "name": "directive definition",
      "scope": [
        "entity.name.other.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#968ba2"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword",
        "keyword.control.directive",
        "storage.modifier.async",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#a2707e"
      }
    },
    {
      "name": "components",
      "scope": [
        "support.class.component"
      ],
      "settings": {
        "foreground": "#628fc3"
      }
    },
    {
      "scope": [
        "storage",
        "keyword.const.go",
        "keyword.type.go",
        "keyword.struct.go",
        "keyword.function.go",
        "keyword.other.declaration-specifier",
        "entity.name.tag",
        "entity.name.section.markdown",
        "entity.name.section.group-title"
      ],
      "settings": {
        "foreground": "#4499b3"
      }
    },
    {
      "name": "typings",
      "scope": [
        "support.type",
        "entity.name.type",
        "keyword.other.type"
      ],
      "settings": {
        "foreground": "#33b28c"
      }
    },
    {
      "name": "functions",
      "scope": [
        "support.function",
        "entity.name.function",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#8c7fab"
      }
    },
    {
