{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ocean Green: Dark",
  "type": "dark",
  "colors": {
    "foreground": "#698d91",
    "disabledForeground": "#698d91",
    "descriptionForeground": "#427277",
    "icon.foreground": "#698d91",
    "focusBorder": "#00000000",
    "progressBar.background": "#4cc48c",
    "selection.background": "#283f41",

    "textLink.activeForeground": "#4cc48cbb",
    "textLink.foreground": "#4cc48c",
    "textBlockQuote.border": "#4cc48c",
    "textBlockQuote.background": "#0f1718",
    "textCodeBlock.background": "#0f1718",
    "textPreformat.foreground": "#4cc48c",
    "textSeparator.foreground": "#192527",

    // links in editor like 'debug' in package.json
    "editorLink.activeForeground": "#4cc48c",

    "editorUnicodeHighlight.background": "#00000000",
    "editorUnicodeHighlight.border": "#b49a43",

    // Scrollbar
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.background": "#213436",
    "scrollbarSlider.activeBackground": "#233739",
    "scrollbarSlider.hoverBackground": "#253a3c",
    "editorOverviewRuler.border": "#00000000",

    // Window, Titlebar & Tabs
    // window.*Border doesn't work on windows 11
    "window.activeBorder": "#162123",
    "window.inactiveBorder": "#162123",

    "titleBar.activeBackground": "#152122",
    "titleBar.inactiveBackground": "#152122",
    "titleBar.activeForeground": "#698d91",
    "titleBar.inactiveForeground": "#698d91",
    "titleBar.border": "#182627",

    "menubar.selectionForeground": "#4cc48c",
    "menubar.selectionBackground": "#00000000",

    "tab.activeBackground": "#00000000",
    "tab.activeForeground": "#4cc48c",
    "tab.activeBorder": "#4cc48c",
    "tab.activeBorderTop": "#4cc48c",
    "tab.border": "#00000000",
    "tab.hoverForeground": "#4cc48c",
    "tab.inactiveForeground": "#698d91",
    "tab.inactiveBackground": "#00000000",
    "editorGroupHeader.noTabsBackground": "#00000000",
    "editorGroupHeader.tabsBackground": "#00000000",
    "editorGroupHeader.border": "#00000000",

    "breadcrumb.background": "#00000000",
    "breadcrumb.foreground": "#698d91",
    "breadcrumb.focusForeground": "#4cc48c",
    "breadcrumb.activeSelectionForeground": "#4cc48c",

    // Activity & Side Bars
    "activityBar.background": "#101819",
    "activityBar.foreground": "#4cc48c",
    "activityBar.border": "#141e1f",
    "activityBar.activeBackground": "#00000000",
    "activityBar.dropBorder": "#4cc48c",
    "activityBar.activeBorder": "#4cc48c",
    "activityBar.activeFocusBorder": "#4cc48c",
    "activityBar.inactiveForeground": "#698d91",

    "sideBar.background": "#121b1c",
    "sideBar.border": "#162123",
    "sideBarTitle.foreground": "#4cc48c",
    "sideBarSectionHeader.background": "#00000000",
    "editorPane.background": "#0f1718",

    "tree.indentGuidesStroke": "#1b2627",
    "tree.tableColumnsBorder": "#1b2627",

    // Widgets, List & Menu
    "widget.shadow": "#00000040",
    "editorWidget.foreground": "#698d91",
    "editorWidget.background": "#172526",
    "editorWidget.border": "#1d2e2f",
    "debugToolBar.background": "#172526",
    "debugToolBar.border": "#1d2e2f",

    // List widget titlebar
    "quickInputTitle.background": "#1d2d2e",

    "list.activeSelectionForeground": "#4cc48c",
    "list.activeSelectionBackground": "#192627",
    "list.inactiveSelectionForeground": "#4cc48c",
    "list.inactiveSelectionBackground": "#192627",
    "list.hoverForeground": "#4cc48c",
    "list.hoverBackground": "#192627",
    "list.focusOutline": "#1b2627",
    "list.focusBackground": "#192627",

    "listFilterWidget.background": "#192627",
    "listFilterWidget.noMatchesOutline": "#192527",
    "listFilterWidget.outline": "#4cc48c2d",

    "list.filterMatchBorder": "#4cc48c",
    "list.filterMatchBackground": "#00000000",

    // F1 menu separator
    "pickerGroup.border": "#192527",
    "pickerGroup.foreground": "#4cc48c",

    "menu.foreground": "#698d91",
    "menu.background": "#172526",
    "menu.border": "#1d2e2f",
    "menu.separatorBackground": "#1d2e2f",
    "menu.selectionForeground": "#4cc48c",
    "menu.selectionBackground": "#1b2b2c",

    // Editor
    "editor.foreground": "#86b4b6",
    "editor.background": "#141f20",

    "editorGroup.border": "#162123",
    "editorBracketMatch.border": "#223233",

    "editorCursor.foreground": "#4cc48c",
    "editorWhitespace.foreground": "#1b2627",
    "editorLineNumber.foreground": "#698d9140",
    "editorLineNumber.activeForeground": "#4cc48c",

    "editor.selectionBackground": "#283f41",
    "editor.lineHighlightBackground": "#151f20",
    "editor.lineHighlightBorder": "#00000000",
    "editor.wordHighlightBackground": "#1a2729",
    "editor.wordHighlightBorder": "#233436",
    "editor.wordHighlightStrongBackground": "#1a2729",
    "editor.wordHighlightStrongBorder": "#2a3e41",
    "editor.rangeHighlightBackground": "#151f20",
    "editor.findMatchBackground": "#14393d",
    "editor.findMatchHighlightBackground": "#14393d",
    "editor.findMatchHighlightBorder": "#4cc48c",
    "editor.findRangeHighlightBackground": "#14393d",
    "editor.findRangeHighlightBorder": "#4cc48c",

    "peekView.border": "#1c2c30",
    "peekViewEditor.background": "#101819",
    "peekViewEditor.matchHighlightBackground": "#14393d",
    "peekViewResult.background": "#101819",
    "peekViewResult.matchHighlightBackground": "#14393d",
    "peekViewResult.lineForeground": "#86b4b6",
    "peekViewTitle.background": "#141f20",
    "peekViewTitleLabel.foreground": "#4cc48c",
    "peekViewTitleDescription.foreground": "#698d91",

    "diffEditor.border": "#162123",
    "diffEditor.diagonalFill": "#162123",
    "diffEditor.insertedLineBackground": "#4f724f25",
    "diffEditor.insertedTextBackground": "#4f724f25",
    "diffEditor.removedLineBackground": "#94313125",
    "diffEditor.removedTextBackground": "#94313125",

    "minimap.selectionHighlight": "#283f41",
    "minimap.findMatchHighlight": "#4cc48c",
    "editorOverviewRuler.findMatchForeground": "#14393d",

    "panel.border": "#162123",

    "statusBar.foreground": "#698d91",
    "statusBar.background": "#152122",
    "statusBar.border": "#182627",
    "statusBar.noFolderForeground": "#698d91",
    "statusBar.noFolderBackground": "#152122",
    "statusBar.noFolderBorder": "#1b2b2c",
    "statusBar.debuggingForeground": "#fff",
    "statusBar.debuggingBackground": "#614a60",
    "statusBar.debuggingBorder": "#755974",

    // Inputs
    "button.background": "#4cc48c8e",
    "button.secondaryBackground": "#1e2c2e",

    "extensionIcon.starForeground": "#c4a259",
    "extensionIcon.sponsorForeground": "#b66eb6",

    "badge.background": "#0f633c",
    "activityBarBadge.background": "#0f633c",

    "inputOption.activeBackground": "#00000000",
    "inputOption.activeForeground": "#4cc48c",
    "inputOption.hoverBackground": "#1f2d30",

    "input.background": "#192527",
    "input.border": "#2a3f41",
    "input.foreground": "#8ea6a8",
    "input.placeholderForeground": "#446d70",

    "dropdown.background": "#192527",
    "dropdown.border": "#2a3f41",
    "dropdown.foreground": "#8ea6a8",
    "dropdown.listBackground": "#121b1b",

    "keybindingLabel.background": "#4cc48c3d",
    "keybindingLabel.border": "#4cc48c3d",
    "keybindingLabel.bottomBorder": "#4cc48c3d",

    // Settings
    "settings.rowHoverBackground": "#151f20",
    "settings.focusedRowBorder": "#2a3f41",
    "settings.focusedRowBackground": "#151f20",
    "settings.sashBorder": "#162123",
    "settings.headerForeground": "#7a9ca0",
    "settings.modifiedItemIndicator": "#4cc48c",

    "gitDecoration.ignoredResourceForeground": "#2c6970",
    "gitDecoration.addedResourceForeground": "#6aa85d",
    "gitDecoration.deletedResourceForeground": "#a85d5d",
    "gitDecoration.modifiedResourceForeground": "#b3915f",
    "gitDecoration.untrackedResourceForeground": "#7580b1",
    "editorGutter.addedBackground": "#6aa85d",
    "editorGutter.deletedBackground": "#a85d5d",
    "editorGutter.modifiedBackground": "#b3915f",

    // Terminal
    "panelTitle.activeForeground": "#4cc48c",
    "panelTitle.activeBorder": "#00000000",
    "panelTitle.inactiveForeground": "#698d91",
    "terminalCursor.foreground": "#4cc48c",
    "terminalOverviewRuler.findMatchForeground": "#14393d",
    "terminalOverviewRuler.cursorForeground": "#4cc48c",
    "terminal.selectionBackground": "#172122",
    "terminal.findMatchBackground": "#14393d",
    "terminal.findMatchBorder": "#4cc48c",
    "terminal.findMatchHighlightBackground": "#14393d",
    "terminal.tab.activeBorder": "#4cc48c",
    "terminal.foreground": "#698d91",
    "terminal.background": "#0f1718",
    "terminal.ansiYellow": "#b9aa5b",
    "terminal.ansiBrightYellow": "#b9aa5b",
    "terminal.ansiRed": "#d66161",
    "terminal.ansiBrightRed": "#f56161",
    "terminal.ansiGreen": "#81a552",
    "terminal.ansiBrightGreen": "#9cd156",
    "terminal.ansiBlue": "#688ec0",
    "terminal.ansiBrightBlue": "#6b9ad8",
    "terminal.ansiCyan": "#4ac5c5",
    "terminal.ansiBrightCyan": "#4ac5c5",
    "terminal.ansiMagenta": "#bb65bb",
    "terminal.ansiBrightMagenta": "#e04be0",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightBlack": "#272727",
    "terminal.ansiWhite": "#dddddd",
    "terminal.ansiBrightWhite": "#fff"
  },
  "tokenColors": [
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#d35151"
      }
    },
    {
      "scope": [
        "meta.brace",
        "punctuation",
        "keyword.operator.class",
        "keyword.operator.type.annotation",
        "keyword.operator.assignment",
        "keyword.operator.optional",
        "keyword.operator.non-null.assertion",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#3c5358"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#2c6970"
      }
    },
    {
      "name": "directives",
      "scope": [
        "keyword.control.directive"
      ],
      "settings": {
        "foreground": "#685157"
      }
    },
    {
      "name": "directive definition",
      "scope": [
        "entity.name.other.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#7a6b63"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword",
        "storage.modifier.async",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#a86e7d"
      }
    },
    {
      "scope": [
        "storage",
        "keyword.const.go",
        "keyword.type.go",
        "keyword.struct.go",
        "keyword.function.go",
        "keyword.other.declaration-specifier",
        "support.class.component",
        "entity.name.tag",
        "entity.name.section.markdown",
        "entity.name.section.group-title"
      ],
      "settings": {
        "foreground": "#3b99b6"
      }
    },
    {
      "name": "typings",
      "scope": [
        "support.type",
        "entity.name.type",
        "keyword.other.type"
      ],
      "settings": {
        "foreground": "#1da37b"
      }
    },
    {
      "name": "functions",
      "scope": [
        "support.function",
        "entity.name.function",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#9587bd"
      }
    },
    {
      "name": "classes",
      "scope": [
        "support.class",
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.type.module",
        "entity.other.alias",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#00bbab"
      }
    },
    {
      "scope": [
        "support.other.namespace",
        "entity.name.type.namespace"
      ],
      "settings": {
        "foreground": "#ad927b"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#a09747"
      }
    },
    {
      "name": "strings",
      "scope": [
        "string",
        "markup.inline.raw.string",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#85a35e"
      }
    },
    {
      "scope": [
        "string.interpolated",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#97a067"
      }
    },
    {
      "scope": [
        "constant",
        "keyword.other.unit.hexadecimal",
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#a8985e"
      }
    },
    {
      "scope": [
        "keyword.other.unit.px",
        "storage.type.numeric.bigint"
      ],
      "settings": {
        "foreground": "#c28a5c"
      }
    },
    {
      "name": "variables",
      "scope": [
        "variable",
        "support.variable",
        "constant.other.caps",
        "storage.modifier.package.java",
        "storage.modifier.import.java"
      ],
      "settings": {
        "foreground": "#86b4b6"
      }
    },
    {
      "name": "this/self",
      "scope": [
        "variable.language.self",
        "variable.language.this",
        "variable.language.special.self",
        "variable.language.special.this"
      ],
      "settings": {
        "foreground": "#6c8d9b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.language.global"
      ],
      "settings": {
        "foreground": "#6c8d9b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "attributes/values",
      "scope": [
        "entity.name.tag.yaml",
        "entity.other.attribute-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#8e739c"
      }
    },
    {
      "scope": [
        "markup.inserted.diff",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#60a060"
      }
    },
    {
      "scope": [
        "meta.diff.header.from-file",
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#bd6e6e"
      }
    },
    {
      "name": "italic style",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    }
  ]
}
