{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ocean Green: Dark",
  "type": "dark",
  "colors": {
    "focusBorder": "#00000000",
    "foreground": "#84b1b4",
    "disabledForeground": "#3b5f61",
    "descriptionForeground": "#566e70",
    "errorForeground": "#d35151",
    "progressBar.background": "#4cc48c",
    
    // #region Window
    "window.activeBorder": "#203132",
    "window.inactiveBorder": "#203132",
    // #endregion
    
    // #region Scrollbar & Minimap
    "scrollbar.shadow": "#101819",
    "scrollbarSlider.background": "#25383a",
    "scrollbarSlider.activeBackground": "#1e2e2f",
    "scrollbarSlider.hoverBackground": "#2c4345",

    "editorOverviewRuler.border": "#00000000",
    "editorOverviewRuler.background": "#1b2829",
    "editorOverviewRuler.addedForeground": "#66a76b",
    "editorOverviewRuler.deletedForeground": "#a76666",
    "editorOverviewRuler.modifiedForeground": "#9975b1",
    "editorOverviewRuler.warningForeground": "#b9aa5b",
    "editorOverviewRuler.errorForeground": "#d35151",
    "editorOverviewRuler.findMatchForeground": "#4cc48c60",
    
    "minimap.background": "#1b2829e3",
    "minimap.errorHighlight": "#862828",
    "minimap.warningHighlight": "#866928",
    "minimap.findMatchHighlight": "#4cc48c60",
    "minimap.selectionHighlight": "#334c4e",
    "minimap.selectionOccurrenceHighlight": "#334c4e",
    "minimapSlider.background": "#25383aad",
    "minimapSlider.activeBackground": "#1e2e2fcb",
    "minimapSlider.hoverBackground": "#2c4345ad",
    // #endregion
    
    // #region Title Bar & Status Bar
    "titleBar.activeForeground": "#84b1b4",
    "titleBar.activeBackground": "#192627",
    "titleBar.inactiveForeground": "#84b1b4",
    "titleBar.inactiveBackground": "#192627",
    "titleBar.border": "#203132",
    
    "statusBar.foreground": "#84b1b4",
    "statusBar.background": "#192627",
    "statusBar.border": "#203132",
    "statusBar.noFolderForeground": "#84b1b4",
    "statusBar.noFolderBackground": "#192627",
    "statusBar.noFolderBorder": "#203132",
    "statusBar.debuggingForeground": "#fff",
    "statusBar.debuggingBackground": "#614a60",
    "statusBar.debuggingBorder": "#755974",

    "statusBarItem.hoverBackground": "#203132",
    "statusBarItem.prominentHoverBackground": "#415c3a",
    "statusBarItem.prominentForeground": "#ffffff",
    "statusBarItem.prominentBackground": "#354b2f",
    // #endregion

    // #region Activity Bar
    "activityBar.activeBorder": "#4cc48c",
    "activityBar.foreground": "#4cc48c",
    "activityBar.background": "#101819",
    "activityBar.border": "#1e2e2f",
    "activityBar.dropBorder": "#4cc48c",
    "activityBar.inactiveForeground": "#566e70",
    "activityBarBadge.background": "#4cc48c60",
    // #endregion

    // #region SideBar & List
    "widget.shadow": "#101819",
    "badge.background": "#4cc48c60",
    
    "panel.background": "#141d1f",
    "panel.border": "#1e2e2f",

    "sideBar.foreground": "#5c7679",
    "sideBar.background": "#141d1f",
    "sideBar.border": "#1e2e2f",
    "sideBarTitle.foreground": "#5c7679",
    "sideBarSectionHeader.foreground": "#5c7679",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.border": "#00000000",
    "sideBar.dropBackground": "#1b272a",

    "quickInputTitle.background": "#233637",
    "list.activeSelectionIconForeground": "#84b1b4",
    "list.activeSelectionForeground": "#84b1b4",
    "list.activeSelectionBackground": "#233637",
    "list.inactiveSelectionIconForeground": "#84b1b4",
    "list.inactiveSelectionForeground": "#84b1b4",
    "list.inactiveSelectionBackground": "#233637",
    "list.hoverForeground": "#84b1b4",
    "list.hoverBackground": "#233637",
    "list.filterMatchBackground": "#223334",
    "list.filterMatchBorder": "#4cc48c60",
    "list.errorForeground": "#d35151",
    "list.warningForeground": "#b9aa5b",

    "listFilterWidget.background": "#233637",
    "listFilterWidget.noMatchesOutline": "#233637",
    "listFilterWidget.outline": "#4cc48c",

    // F1 separator
    "pickerGroup.border": "#233637",
    "pickerGroup.foreground": "#84b1b4",

    // Menu
    "menu.foreground": "#84b1b4",
    "menu.background": "#1c2b2c",
    "menu.border": "#233637",
    "menu.separatorBackground": "#233637",
    "menu.selectionBackground": "#233637",

    "debugToolBar.background": "#1c2b2c",
    "debugToolBar.border": "#233637",

    // F1 menu, feedback
    "editorWidget.foreground": "#84b1b4",
    "editorWidget.background": "#1c2b2c",
    "editorWidget.border": "#233637",
    "editorWidget.resizeBorder": "#233637",

    // Tooltip
    "editorHoverWidget.foreground": "#84b1b4",
    "editorHoverWidget.background": "#1c2b2c",
    "editorHoverWidget.border": "#233637",

    "tree.indentGuidesStroke": "#1e2e2f",
    "tree.tableColumnsBorder": "#1e2e2f",
    // #endregion

    // #region Tabs & Breadcrumb
    "tab.border": "#1e2e2f",
    "tab.activeBorder": "#4cc48c",
    "tab.activeBorderTop": "#4cc48c",
    "tab.activeBackground": "#1b2829",
    "tab.activeForeground": "#4cc48c",
    "tab.inactiveForeground": "#566e70",
    "tab.inactiveBackground": "#172324",

    "editorGroup.border": "#1e2e2f",
    "editorGroupHeader.tabsBackground": "#172324",
    "editorGroupHeader.noTabsBackground": "#172324",
    "editorGroupHeader.border": "#1e2e2f",
    "editorGroupHeader.tabsBorder": "#1e2e2f",

    "breadcrumb.background": "#172324",
    // #endregion

    // #region Icons
    "icon.foreground": "#84b1b4",

    "toolbar.hoverOutline": "#00000000",
    "toolbar.activeBackground": "#203132",
    "toolbar.hoverBackground": "#203132",

    "panelTitle.activeForeground": "#4cc48c",
    "panelTitle.activeBorder": "#4cc48c",
    "panelTitle.inactiveForeground": "#84b1b4",

    "inputOption.activeBackground": "#00000000",
    "inputOption.activeBorder": "#00000000",
    "inputOption.activeForeground": "#4cc48c",
    "inputOption.hoverBackground": "#203132",
    // #endregion

    // #region Editor
    "editor.foreground": "#84b1b4",
    "editor.background": "#172324",

    "editorCursor.background": "#4cc48c",
    "editorCursor.foreground": "#4cc48c",

    "editorBracketMatch.border": "#334c4e",
    "editorBracketMatch.background": "#00000000",

    "peekView.border": "#1e2e2f",
    "peekViewEditor.background": "#152021",
    "peekViewEditor.matchHighlightBackground": "#255043",

    "peekViewResult.background": "#172324",
    "peekViewResult.lineForeground": "#84b1b4",
    "peekViewResult.selectionBackground": "#223334",
    "peekViewResult.matchHighlightBackground": "#255043",
    "peekViewResult.fileForeground": "#84b1b4",
    "peekViewTitle.background": "#1b2829",
    "peekViewTitleLabel.foreground": "#84b1b4",
    "peekViewTitleDescription.foreground": "#566e70",

    "editorError.foreground": "#d35151",
    "editorWarning.foreground": "#b9aa5b",

    "editorWhitespace.foreground": "#222c2d",
    "editorLineNumber.activeForeground": "#4cc48c",
    "editorLineNumber.foreground": "#39494b",

    "editor.lineHighlightBorder": "#1b2829",
    "editor.lineHighlightBackground": "#1b2829",
    
    "editor.selectionBackground": "#293d3f",
    "editor.selectionHighlightBackground": "#293d3f",
    
    "editor.wordHighlightBackground": "#293d3f",
    "editor.wordHighlightStrongBackground": "#365153",
    
    "editor.rangeHighlightBackground": "#1b2829",

    "editor.findMatchBorder": "#4cc48c",
    "editor.findMatchBackground": "#4cc48c57",
    "editor.findMatchHighlightBackground": "#4cc48c57",
    "editor.findRangeHighlightBackground": "#4cc48c57",

    "editorUnicodeHighlight.background": "#00000000",
    "editorUnicodeHighlight.border": "#b49a43",

    "editorLink.activeForeground": "#81568b",
    "debugIcon.breakpointForeground": "#81568b",
    // #endregion

    // #region Extensions
    "extensionIcon.starForeground": "#aca54a",
    "extensionIcon.preReleaseForeground": "#4cc48c60",
    // #endregion

    // #region Inputs
    "commandCenter.activeBackground": "#101819",
    "commandCenter.background": "#101819",
    "commandCenter.border": "#25383a",
    "commandCenter.foreground": "#566e70",

    "button.foreground": "#ffffff",
    "button.background": "#4cc48c60",
    "button.separator": "#4cc48c65",
    "button.secondaryForeground": "#ffffff",
    "button.secondaryBackground": "#334e50",
    "button.secondaryHoverBackground": "#3a585b",

    "keybindingLabel.border": "#4cc48c65",
    "keybindingLabel.bottomBorder": "#4cc48c65",
    "keybindingLabel.foreground": "#ffffff",
    "keybindingLabel.background": "#4cc48c60",

    "input.foreground": "#84b1b4",
    "input.background": "#101819",
    "input.border": "#25383a",
    "input.placeholderForeground": "#566e70",

    "dropdown.foreground": "#84b1b4",
    "dropdown.background": "#101819",
    "dropdown.border": "#25383a",
    "dropdown.listBackground": "#101819",

    "inputValidation.infoForeground": "#ffffff",
    "inputValidation.infoBorder": "#2b8692",
    "inputValidation.infoBackground": "#287b86",
    "inputValidation.errorForeground": "#ffffff",
    "inputValidation.errorBorder": "#922b2b",
    "inputValidation.errorBackground": "#862828",
    "inputValidation.warningForeground": "#ffffff",
    "inputValidation.warningBorder": "#92722b",
    "inputValidation.warningBackground": "#866928",

    "textLink.activeForeground": "#4cc48c",
    "textLink.foreground": "#4cc48c",
    "textBlockQuote.background": "#101819",
    "textBlockQuote.border": "#4cc48c",
    "textPreformat.foreground": "#4cc48c",
    "textSeparator.foreground": "#233637",
    // #endregion

    // #region Settings
    "settings.headerForeground": "#4cc48c",
    "settings.headerBorder": "#1e2e2f",
    "settings.focusedRowBackground": "#1e2e2f",
    "settings.focusedRowBorder": "#25383a",
    "settings.rowHoverBackground": "#1c2b2c",
    "settings.modifiedItemIndicator": "#4cc48c",
    // #endregion

    // #region Git Decoration
    "editorGutter.modifiedBackground": "#9975b1",
    "editorGutter.deletedBackground": "#a76666",
    "editorGutter.addedBackground": "#66a76b",

    "gitDecoration.ignoredResourceForeground": "#39494b",
    "gitDecoration.modifiedResourceForeground": "#9975b1",
    "gitDecoration.deletedResourceForeground": "#a76666",
    "gitDecoration.addedResourceForeground": "#66a76b",
    "gitDecoration.conflictingResourceForeground": "#a77e66",
    "gitDecoration.renamedResourceForeground": "#8ea766",
    "gitDecoration.untrackedResourceForeground": "#7580b1",
    
    // Diff
    "diffEditor.border": "#162123",
    "diffEditor.diagonalFill": "#162123",
    "diffEditor.insertedLineBackground": "#4f724f25",
    "diffEditor.insertedTextBackground": "#4f724f25",
    "diffEditor.removedLineBackground": "#94313125",
    "diffEditor.removedTextBackground": "#94313125",
    // #endregion

    // #region Terminal
    "terminalCursor.background": "#4cc48c",
    "terminalCursor.foreground": "#4cc48c",
    "terminal.foreground": "#84b1b4",
    "terminal.ansiYellow": "#b9aa5b",
    "terminal.ansiBrightYellow": "#b9aa5b",
    "terminal.ansiRed": "#d66161",
    "terminal.ansiBrightRed": "#f56161",
    "terminal.ansiGreen": "#81a552",
    "terminal.ansiBrightGreen": "#9cd156",
    "terminal.ansiBlue": "#688ec0",
    "terminal.ansiBrightBlue": "#6b9ad8",
    "terminal.ansiCyan": "#4ac5c5",
    "terminal.ansiBrightCyan": "#4ac5c5",
    "terminal.ansiMagenta": "#bb65bb",
    "terminal.ansiBrightMagenta": "#e04be0",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightBlack": "#272727",
    "terminal.ansiWhite": "#dddddd",
    "terminal.ansiBrightWhite": "#fff"
    // #endregion
  },
  "tokenColors": [
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#d35151"
      }
    },
    {
      "scope": [
        "meta.brace",
        "punctuation",
        "keyword.operator.class",
        "keyword.operator.type.annotation",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#3c5358"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
      ],
      "settings": {
        "foreground": "#2c6970"
      }
    },
    {
      "name": "directives",
      "scope": [
        "keyword.control.directive",
      ],
      "settings": {
        "foreground": "#685157"
      }
    },
    {
      "name": "directive definition",
      "scope": [
        "entity.name.other.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#7a6b63"
      }
    },
    {
      "name": "keywords",
      "scope": [
        "keyword",
        "storage.modifier.async",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#be6e83"
      }
    },
    {
      "scope": [
        "storage",
        "keyword.const.go",
        "keyword.type.go",
        "keyword.struct.go",
        "keyword.function.go",
        "keyword.other.declaration-specifier",
        "support.class.component",
        "entity.name.tag",
        "entity.name.section.markdown",
        "entity.name.section.group-title"
      ],
      "settings": {
        "foreground": "#37a5c7"
      }
    },
    {
      "name": "typings",
      "scope": [
        "support.type",
        "entity.name.type",
        "keyword.other.type"
      ],
      "settings": {
        "foreground": "#1dc08f"
      }
    },
    {
      "name": "functions",
      "scope": [
        "support.function",
        "entity.name.function",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#9587bd"
      }
    },
    {
      "name": "classes",
      "scope": [
        "support.class",
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.type.module",
        "entity.other.alias",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#00bbab"
      }
    },
    {
      "scope": [
        "support.other.namespace",
        "entity.name.type.namespace"
      ],
      "settings": {
        "foreground": "#ad927b"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#a09747"
      }
    },
    {
      "name": "strings",
      "scope": [
        "string",
        "markup.inline.raw.string",
        "markup.fenced_code.block"
      ],
      "settings": {
        "foreground": "#85a35e"
      }
    },
    {
      "scope": [
        "string.interpolated",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#97a067"
      }
    },
    {
      "scope": [
        "constant",
        "keyword.other.unit.hexadecimal",
        "fenced_code.block.language"
      ],
      "settings": {
        "foreground": "#ad9d60"
      }
    },
    {
      "scope": [
        "keyword.other.unit.px",
        "storage.type.numeric.bigint"
      ],
      "settings": {
        "foreground": "#c28a5c"
      }
    },
    {
      "name": "variables",
      "scope": [
        "variable",
        "support.variable",
        "constant.other.caps",
        "storage.modifier.package.java",
        "storage.modifier.import.java"
      ],
      "settings": {
        "foreground": "#84b1b4"
      }
    },
    {
      "name": "this/self",
      "scope": [
        "variable.language.self",
        "variable.language.this",
        "variable.language.special.self",
        "variable.language.special.this"
      ],
      "settings": {
        "foreground": "#6c8d9b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.language.global"
      ],
      "settings": {
        "foreground": "#6c8d9b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "attributes/values",
      "scope": [
        "entity.name.tag.yaml",
        "entity.other.attribute-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#8e739c"
      }
    },
    {
      "scope": [
        "markup.inserted.diff",
        "meta.diff.header.to-file"
      ],
      "settings": {
        "foreground": "#60a060"
      }
    },
    {
      "scope": [
        "meta.diff.header.from-file",
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "#bd6e6e"
      }
    },
    {
      "name": "italic style",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "bold",
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    }
  ]
}
