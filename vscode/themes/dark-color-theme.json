{
  "$schema": "vscode://schemas/color-theme",
  "name": "OceanGreen: Dark",
  "type": "dark",
  "colors": {
    "foreground": "#577477",
    "disabledForeground": "#577477",
    "descriptionForeground": "#427277",
    "icon.foreground": "#577477",
    "focusBorder": "#00000000",
    "progressBar.background": "#4cc48c",
    "selection.background": "#283f41",

    "textLink.activeForeground": "#4cc48cbb",
    "textLink.foreground": "#4cc48c",
    "textBlockQuote.border": "#4cc48c",
    "textBlockQuote.background": "#0f1718",
    "textCodeBlock.background": "#0f1718",
    "textPreformat.foreground": "#4cc48c",
    "textSeparator.foreground": "#192527",

    // links in editor like 'debug' in package.json
    "editorLink.activeForeground": "#4cc48c",

    "editorUnicodeHighlight.background": "#00000000",
    "editorUnicodeHighlight.border": "#b49a43",

    // Scrollbar
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.background": "#1f2e30",
    "scrollbarSlider.activeBackground": "#243638",
    "scrollbarSlider.hoverBackground": "#243638",
    "editorOverviewRuler.border": "#00000000",

    // Window, Titlebar & Tabs
    // window.*Border doesn't work on windows 11
    "window.activeBorder": "#131d1f",
    "window.inactiveBorder": "#131d1f",

    "titleBar.activeBackground": "#111a1b",
    "titleBar.inactiveBackground": "#111a1b",
    "titleBar.activeForeground": "#577477",
    "titleBar.inactiveForeground": "#577477",
    "titleBar.border": "#131d1f",

    "menubar.selectionForeground": "#4cc48c",
    "menubar.selectionBackground": "#00000000",

    "tab.activeForeground": "#4cc48c",
    "tab.activeBorder": "#4cc48c",
    "tab.activeBorderTop": "#4cc48c",
    "tab.border": "#00000000",
    "tab.hoverForeground": "#4cc48c",
    "tab.inactiveForeground": "#577477",
    "tab.inactiveBackground": "#00000000",
    "editorGroupHeader.noTabsBackground": "#00000000",
    "editorGroupHeader.tabsBackground": "#00000000",
    "editorGroupHeader.border": "#131d1f",

    "breadcrumb.foreground": "#577477",
    "breadcrumb.focusForeground": "#4cc48c",
    "breadcrumb.activeSelectionForeground": "#4cc48c",

    // Activity & Side Bars
    "activityBar.background": "#111a1b",
    "activityBar.foreground": "#4cc48c",
    "activityBar.border": "#131d1f",
    "activityBar.activeBackground": "#0f1718",
    "activityBar.dropBorder": "#4cc48c",
    "activityBar.activeBorder": "#4cc48c",
    "activityBar.activeFocusBorder": "#4cc48c",
    "activityBar.inactiveForeground": "#577477",

    "sideBar.background": "#0f1718",
    "sideBar.border": "#131d1f",
    "sideBarTitle.foreground": "#4cc48c",
    "sideBarSectionHeader.background": "#00000000",
    "editorPane.background": "#0f1718",

    "tree.indentGuidesStroke": "#1b2627",
    "tree.tableColumnsBorder": "#1b2627",

    // Widgets, List & Menu
    "widget.shadow": "#00000040",
    "editorWidget.foreground": "#768e91",
    "editorWidget.background": "#162122",
    "editorWidget.border": "#192527",

    // List widget titlebar
    "quickInputTitle.background": "#1d2d2e",

    "list.activeSelectionForeground": "#4cc48c",
    "list.activeSelectionBackground": "#192627",
    "list.inactiveSelectionForeground": "#4cc48c",
    "list.inactiveSelectionBackground": "#192627",
    "list.hoverForeground": "#4cc48c",
    "list.hoverBackground": "#192627",
    "list.focusOutline": "#1b2627",
    "list.focusBackground": "#192627",

    "listFilterWidget.background": "#192627",
    "listFilterWidget.noMatchesOutline": "#192527",
    "listFilterWidget.outline": "#4cc48c2d",

    "list.filterMatchBorder": "#4cc48c",
    "list.filterMatchBackground": "#00000000",

    // F1 menu separator
    "pickerGroup.border": "#192527",
    "pickerGroup.foreground": "#4cc48c",

    "menu.foreground": "#768e91",
    "menu.background": "#162122",
    "menu.border": "#192527",
    "menu.separatorBackground": "#192527",
    "menu.selectionForeground": "#4cc48c",
    "menu.selectionBackground": "#1e2d2e",

    // Editor
    "editor.foreground": "#8ba6a7",
    "editor.background": "#111a1b",

    "editorGroup.border": "#131d1f",
    "editorBracketMatch.border": "#223233",

    "editorCursor.foreground": "#4cc48c",
    "editorWhitespace.foreground": "#1b2627",
    "editorLineNumber.foreground": "#223233",
    "editorLineNumber.activeForeground": "#4cc48c",

    "editor.selectionBackground": "#283f41",
    "editor.lineHighlightBackground": "#151f20",
    "editor.lineHighlightBorder": "#00000000",
    "editor.wordHighlightBackground": "#1a2729",
    "editor.wordHighlightBorder": "#233436",
    "editor.wordHighlightStrongBackground": "#1a2729",
    "editor.wordHighlightStrongBorder": "#2a3e41",
    "editor.rangeHighlightBackground": "#151f20",
    "editor.findMatchBackground": "#14393d",
    "editor.findMatchHighlightBackground": "#14393d",
    "editor.findMatchHighlightBorder": "#4cc48c",
    "editor.findRangeHighlightBackground": "#14393d",
    "editor.findRangeHighlightBorder": "#4cc48c",

    "peekView.border": "#1c2c30",
    "peekViewEditor.background": "#111a1b",
    "peekViewEditor.matchHighlightBackground": "#14393d",
    "peekViewResult.background": "#111a1b",
    "peekViewResult.matchHighlightBackground": "#14393d",
    "peekViewTitle.background": "#141f20",
    "peekViewTitleLabel.foreground": "#4cc48c",
    "peekViewTitleDescription.foreground": "#577477",
    "peekViewResult.lineForeground": "#8ba6a7",

    "diffEditor.border": "#131d1f",
    "diffEditor.diagonalFill": "#131d1f",
    "diffEditor.insertedLineBackground": "#4f724f25",
    "diffEditor.insertedTextBackground": "#4f724f25",
    "diffEditor.removedLineBackground": "#94313125",
    "diffEditor.removedTextBackground": "#94313125",

    "minimap.selectionHighlight": "#283f41",
    "minimap.findMatchHighlight": "#4cc48c",
    "editorOverviewRuler.findMatchForeground": "#14393d",

    "panel.border": "#131d1f",

    "statusBar.foreground": "#577477",
    "statusBar.background": "#111a1b",
    "statusBar.border": "#131d1f",
    "statusBar.noFolderForeground": "#577477",
    "statusBar.noFolderBackground": "#111a1b",
    "statusBar.noFolderBorder": "#131d1f",

    // Inputs
    "button.background": "#4cc48c8e",
    "button.secondaryBackground": "#1e2c2e",

    "extensionSponsorButton.background": "#613f5e",
    "extensionSponsorButton.hoverBackground": "#724a6f",

    "badge.background": "#4cc48c5e",
    "activityBarBadge.background": "#4cc48c5e",

    "inputOption.activeBackground": "#00000000",
    "inputOption.activeForeground": "#4cc48c",
    "inputOption.hoverBackground": "#1f2d30",

    "input.background": "#192527",
    "input.border": "#2a3f41",
    "input.foreground": "#8ea6a8",
    "input.placeholderForeground": "#446d70",

    "dropdown.background": "#192527",
    "dropdown.border": "#2a3f41",
    "dropdown.foreground": "#8ea6a8",
    "dropdown.listBackground": "#121b1b",

    "keybindingLabel.background": "#4cc48c3d",
    "keybindingLabel.border": "#4cc48c3d",
    "keybindingLabel.bottomBorder": "#4cc48c3d",

    // Settings
    "settings.rowHoverBackground": "#151f20",
    "settings.focusedRowBorder": "#2a3f41",
    "settings.focusedRowBackground": "#151f20",
    "settings.sashBorder": "#131d1f",
    "settings.headerForeground": "#7a9ca0",
    "settings.modifiedItemIndicator": "#4cc48c",

    // Terminal
    "panelTitle.activeForeground": "#4cc48c",
    "panelTitle.activeBorder": "#00000000",
    "panelTitle.inactiveForeground": "#577477",
    "terminalCursor.foreground": "#4cc48c",
    "terminalOverviewRuler.findMatchForeground": "#14393d",
    "terminalOverviewRuler.cursorForeground": "#4cc48c",
    "terminal.selectionBackground": "#172122",
    "terminal.findMatchBackground": "#14393d",
    "terminal.findMatchBorder": "#4cc48c",
    "terminal.findMatchHighlightBackground": "#14393d",
    "terminal.tab.activeBorder": "#4cc48c",
    "terminal.foreground": "#577477",
    "terminal.background": "#0f1718",
    "terminal.ansiYellow": "#b9aa5b",
    "terminal.ansiBrightYellow": "#b9aa5b",
    "terminal.ansiRed": "#d66161",
    "terminal.ansiBrightRed": "#f56161",
    "terminal.ansiGreen": "#81a552",
    "terminal.ansiBrightGreen": "#9cd156",
    "terminal.ansiBlue": "#688ec0",
    "terminal.ansiBrightBlue": "#6b9ad8",
    "terminal.ansiCyan": "#4ac5c5",
    "terminal.ansiBrightCyan": "#4ac5c5",
    "terminal.ansiMagenta": "#bb65bb",
    "terminal.ansiBrightMagenta": "#e04be0",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBrightBlack": "#272727",
    "terminal.ansiWhite": "#dddddd",
    "terminal.ansiBrightWhite": "#fff"
  },
  "tokenColors": [
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#d35151"
      }
    },
    {
      "scope": [
        "meta.brace",
        "punctuation",
        "keyword.operator.class",
        "keyword.operator.type.annotation",
        "keyword.operator.assignment",
        "keyword.operator.optional",
        "keyword.operator.non-null.assertion",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#3c5358"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#2c6970"
      }
    },
    {
      "name": "directives",
      "scope": ["keyword.control.directive"],
      "settings": {
        "foreground": "#685157"
      }
    },
    {
      "name": "directive definition",
      "scope": ["entity.name.other.preprocessor", "entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#7a6b63"
      }
    },
    {
      "name": "keywords",
      "scope": ["keyword", "storage.modifier.async", "storage.type.function"],
      "settings": {
        "foreground": "#a86e7d"
      }
    },
    {
      "scope": [
        "storage",
        "keyword.const.go",
        "keyword.type.go",
        "keyword.struct.go",
        "keyword.function.go",
        "keyword.other.declaration-specifier",
        "support.class.component",
        "entity.name.tag",
        "entity.name.section.markdown",
        "entity.name.section.group-title"
      ],
      "settings": {
        "foreground": "#3b99b6"
      }
    },
    {
      "name": "typings",
      "scope": ["support.type", "entity.name.type", "keyword.other.type"],
      "settings": {
        "foreground": "#2cad91"
      }
    },
    {
      "name": "functions",
      "scope": ["support.function", "entity.name.function", "variable.language.super"],
      "settings": {
        "foreground": "#867ca5"
      }
    },
    {
      "name": "classes",
      "scope": [
        "support.class",
        "entity.name.class",
        "entity.name.type.class",
        "entity.name.type.module",
        "entity.other.alias",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#00bbab"
      }
    },
    {
      "scope": ["support.other.namespace", "entity.name.type.namespace"],
      "settings": {
        "foreground": "#ad927b"
      }
    },
    {
      "scope": ["entity.other.attribute-name.id", "entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#a09747"
      }
    },
    {
      "name": "strings",
      "scope": ["string", "markup.inline.raw.string", "markup.fenced_code.block"],
      "settings": {
        "foreground": "#81a552"
      }
    },
    {
      "scope": ["string.interpolated", "markup.underline.link"],
      "settings": {
        "foreground": "#97a067"
      }
    },
    {
      "scope": ["constant", "keyword.other.unit.hexadecimal", "fenced_code.block.language"],
      "settings": {
        "foreground": "#a1925e"
      }
    },
    {
      "scope": ["keyword.other.unit.px", "storage.type.numeric.bigint"],
      "settings": {
        "foreground": "#c28a5c"
      }
    },
    {
      "name": "variables",
      "scope": ["variable", "support.variable", "constant.other.caps", "storage.modifier.package.java", "storage.modifier.import.java"],
      "settings": {
        "foreground": "#8ba6a7"
      }
    },
    {
      "name": "this/self",
      "scope": ["variable.language.self", "variable.language.this", "variable.language.special.self", "variable.language.special.this"],
      "settings": {
        "foreground": "#6c8d9b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.language.global"],
      "settings": {
        "foreground": "#6c8d9b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "attributes/values",
      "scope": ["entity.name.tag.yaml", "entity.other.attribute-name", "support.type.property-name"],
      "settings": {
        "foreground": "#8d7b97"
      }
    },
    {
      "scope": ["markup.inserted.diff", "meta.diff.header.to-file"],
      "settings": {
        "foreground": "#60a060"
      }
    },
    {
      "scope": ["meta.diff.header.from-file", "markup.deleted.diff"],
      "settings": {
        "foreground": "#bd6e6e"
      }
    },
    {
      "name": "italic style",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "bold",
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    }
  ]
}
